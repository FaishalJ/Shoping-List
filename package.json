{
  "name": "react-javascript",
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript on Replit, using Vite bundler",
  "scripts": {
    "watch:sass": "sass src/sass/main.scss src/App.css -w",
    "compile:sass": "sass src/sass/main.scss src/App.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/App.css -o src/App.prefix.css",
    "compress:css": "sass src/App.prefix.css src/App.css --output-App compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  }
}
